<?xml version="1.0"?>
<Project frameworkVersion="v4_5" name="SilverSim.Scripting.Common" path="SilverSim/Scripting.Common" type="Library">
  <Configuration name="Release">
    <Options>
      <OutputPath>../../bin/</OutputPath>
    </Options>
  </Configuration>
  <Configuration name="Debug">
    <Options>
      <OutputPath>../../bin/</OutputPath>
    </Options>
  </Configuration>
  <ReferencePath>../../bin/</ReferencePath>
  <Reference name="log4net" path="../../bin/" localCopy="false"/>
  <Reference name="System"/>
  <Reference name="System.Core"/>
  <Reference name="SilverSim.Types" localCopy="false"/>
  <Reference name="SilverSim.Scene.Types" localCopy="false"/>
  <Reference name="SilverSim.Threading" localCopy="false"/>
  <Reference name="Nini" path="../../bin/" localCopy="false"/>
  <Files>
    <Match pattern="*.cs" recurse="true"/>
    <Match pattern="ProductAssemblyInfo.cs" path=".." recurse="false"/>
    <Match pattern="Prebuild.xml" recurse="false" buildAction="None"/>
    <Match pattern="Product.licenseheader" path=".." recurse="false" buildAction="None"/>
  </Files>
</Project>
